{
    "name": "waveform-viewer-frontend",
    "private": true,
    "license": "MIT",
    "version": "0.3.7",
    "type": "module",
    "workspaces": [
        "packages/core",
        "packages/react-ui",
        "packages/app-web",
        "packages/app-tauri",
        "packages/app-vscode"
    ],
    "scripts": {
        "dev": "npm run dev -w @waveform-viewer/app-web",
        "dev:tauri": "npm run dev -w @waveform-viewer/app-tauri",
        "build": "npm run build -w @waveform-viewer/app-web",
        "build:tauri": "npm run build -w @waveform-viewer/app-tauri",
        "build:vscode": "npm run build -w @waveform-viewer/app-vscode",
        "build:all": "npm run build -w @waveform-viewer/app-web && npm run build -w @waveform-viewer/app-tauri && npm run build -w @waveform-viewer/app-vscode",
        "lint": "eslint .",
        "preview": "npm run preview -w @waveform-viewer/app-web"
    },
    "devDependencies": {
        "@eslint/js": "^10.0.1",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.1",
        "eslint": "^10.0.1",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "globals": "^17.3.0",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.36.0",
        "vite": "^7.3.1"
    },
    "overrides": {
        "minimatch": "^10.2.2"
    }
}